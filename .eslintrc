{
  "parser": "babel-eslint",
  "extends": "standard",
  "plugins": [
    "flow-vars"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "__DEBUG__": false,
    "__DEBUG_NEW_WINDOW__": false,
    "__BASENAME__": false,
    "angular": true,
    "jQuery": true,
    "async": true,
    "$": true
  },
  "rules": {
    "indent": [2,4],
    "semi": [1, "never"],
    "camelcase": 0,
    "space-infix-ops": 0,
    "flow-vars/define-flow-type": 1,
    "flow-vars/use-flow-type": 1,
    "arrow-parens": 0,
    "no-extend-native": 0
  }
}